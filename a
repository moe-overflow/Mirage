[36m@@ -0,0 +1,24 @@[m
[32m+[m[32m#pragma once[m
[32m+[m
[32m+[m[32m#ifdef MIRAGE_PLATFORM_WINDOWS[m
[32m+[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * A function that will be define somewhere else and return the application (CLIENT)[m
[32m+[m[32m *[m
[32m+[m[32m */[m
[32m+[m[32mextern Mirage::Application* Mirage::CreateApplication();[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * main function of program. This is located in the engine itself in order to[m[41m [m
[32m+[m[32m * control the program secuence from here and not in clients[m
[32m+[m[32m *[m
[32m+[m[32m */[m
[32m+[m[32mint main(int argc, char** argv)[m
[32m+[m[32m{[m
[32m+[m	[32mauto app = Mirage::CreateApplication();[m
[32m+[m	[32mapp->Run();[m
[32m+[m	[32mdelete app;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m#endif[m
