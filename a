[32m+[m[32m#pragma once[m
[32m+[m
[32m+[m[32m// macros for importing and exporting dll functions[m
[32m+[m[32m#ifdef MIRAGE_PLATFORM_WINDOWS[m
[32m+[m
[32m+[m	[32m#ifdef MIRAGE_BUILD_DLL[m
[32m+[m		[32m#define MIRAGE_API __declspec(dllexport)[m
[32m+[m	[32m#else[m
[32m+[m		[32m#define MIRAGE_API __declspec(dllimport)[m
[32m+[m	[32m#endif[m
[32m+[m[32m#else[m
[32m+[m	[32m#error Mirage supports currently only windows![m
[32m+[m[32m#endif[m
[1mdiff --git a/Mirage/src/Mirage/EntryPoint.h b/Mirage/src/Mirage/EntryPoint.h[m
